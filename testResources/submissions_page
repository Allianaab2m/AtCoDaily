ust: inherit; font-kerning: inherit; font-feature-settings: inherit; font-variation-settings: inherit; overflow: visible;">XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX</div></div></pre><pre id="for_copy0" class="source-code-for-copy">#include &lt;bits/stdc++.h&gt;
using namespace std;
//{{{
using ll = long long;
ll INF = 2e18;

// Vector
template &lt;typename T&gt; using vc = vector&lt;T&gt;;
template &lt;typename T&gt; using vv = vc&lt;vc&lt;T&gt;&gt;;
using vl = vc&lt;ll&gt;;      // vector&lt;ll&gt;
using vvl = vv&lt;ll&gt;;     // vector&lt;vector&lt;ll&gt;&gt;
using vvvl = vv&lt;vl&gt;;    // vector&lt;vector&lt;vector&lt;ll&gt;&gt;&gt;
using vvvvl = vv&lt;vvl&gt;;  // vector&lt;vector&lt;vector&lt;vector&lt;ll&gt;&gt;&gt;&gt;
using vs = vc&lt;string&gt;;  // vector&lt;string&gt;
using vvs = vv&lt;string&gt;; // vector&lt;vector&lt;string&gt;&gt;

// CE when using brace([]) access in vector
#ifdef ONLINE_JUDGE
#define _GLIBCXX_DEBUG
#define _LIBCPP_DEBUG 0
#endif

#define OVERLOAD_REP(_1, _2, _3, name, ...) name
#define REP1(i, n) for (auto i = std::decay_t&lt;decltype(n)&gt;{}; (i) != (n); ++(i))
#define REP2(i, l, r) for (auto i = (l); (i) != (r); ++(i))
#define rep(...) OVERLOAD_REP(__VA_ARGS__, REP2, REP1)(__VA_ARGS__)

#define all(...) std::begin(__VA_ARGS__), std::end(__VA_ARGS__)
#define rall(...) std::rbegin(__VA_ARGS__), std::rend(__VA_ARGS__)

#define Yes cout &lt;&lt; "Yes" &lt;&lt; '\n'
#define No cout &lt;&lt; "No" &lt;&lt; '\n'
// if (a == b) YN;
#define YN                                                                     \
  {                                                                            \
    cout &lt;&lt; "Yes" &lt;&lt; "\n";                                                     \
  }                                                                            \
  else {                                                                       \
    cout &lt;&lt; "No" &lt;&lt; "\n";                                                      \
  }

#define chmax(x, y) x = max(x, y)
#define chmin(x, y) x = min(x, y)

// Grid
// [0] = r [1] = d [2] = l [3] = u
vl dx = {1, 0, -1, 0};
vl dy = {0, 1, 0, -1};

// Trueの時にContinue
bool out_grid(ll i, ll j, ll h, ll w) {
  return (!(0 &lt;= i &amp;&amp; i &lt; h &amp;&amp; 0 &lt;= j &amp;&amp; j &lt; w));
}

// nC_2, nC_3
ll nc2(ll x) { return x * (x - 1) / 2; }
ll nc3(ll x) { return x * (x - 1) * (x - 2) / 6; }

#define debug(x) cerr &lt;&lt; #x &lt;&lt; " = " &lt;&lt; x &lt;&lt; '\n'

// cin &gt;&gt; a; for vector!
template &lt;class T&gt; istream &amp;operator&gt;&gt;(istream &amp;i, vc&lt;T&gt; &amp;v) {
  rep(j, size(v)) i &gt;&gt; v[j];
  return i;
}
//  }}}

int main() {
  // Code here:
  ll n, k;
  cin &gt;&gt; n &gt;&gt; k;
  string s;
  cin &gt;&gt; s;

  vl idx = {0};
  vs seg;

  rep(i, 1, s.length() + 1) {
    if (s[i - 1] != s[i]) {
      idx.push_back(i);
    }
  }

  rep(i, idx.size()) { seg.push_back(s.substr(idx[i], idx[i + 1] - idx[i])); }

  ll k_1_idx = 0;
  if (s[0] == '0') {
    k_1_idx = k * 2 - 1;
  } else {
    k_1_idx = k * 2 - 2;
  }

  swap(seg[k_1_idx - 1], seg[k_1_idx]);

  rep(i, seg.size()) { cout &lt;&lt; seg[i]; }

  cout &lt;&lt; endl;

  return 0;
}

// vim: foldmethod=marker foldlevel=0
</pre>

